{
  "name": "vue3-localstorage-sync",
  "version": "0.1.1",
  "description": "A simple utility package that synchronizes reactive data between localStorage and different pages in a Vue 3 application.",
  "author": "TerryYoung518",
  "license": "MIT",
  "homepage": "https://github.com/TerryYoung518/vue3-localstorage-sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryYoung518/vue3-localstorage-sync.git"
  },
  "bugs": {
    "url": "https://github.com/TerryYoung518/vue3-localstorage-sync/issues"
  },
  "keywords": [
    "Vue 3",
    "vue",
    "localStorage",
    "sync",
    "reactive data",
    "storage",
    "watch"
  ],
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pack:dry-run": "npm pack --dry-run"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "dependencies": {
    "vue": "^3.4.21"
  }
}
